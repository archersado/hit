    "npm-ensure": "^1.2.0",
    "ts-jest": "^27.0.3",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "is-type-of": "^1.2.1",
    "lodash.get": "^4.4.2"
  }
