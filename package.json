{
  "name": "@hset/hit",
  "version": "1.0.3",
  "description": "a light expression tools for hlang",
  "main": "lib/index.js",
  "type": "lib/index.d.ts",
  "scripts": {
    "test": "tnpm run build && jest --coverage --forceExit",
    "build": "tsc -p tsconfig.json",
    "lint": "tslint --project . -c tslint.json --fix && npm-ensure -t deps",
    "watch": "tsc -p tsconfig.json -w",
    "prepublishOnly": "tnpm run build",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "**/*.ts": [
      "tnpm run lint",
      "git add"
    ]
  },
  "ensure": {
    "deps": {
      "checkDirs": [
        "**/*"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:archersado/hit.git"
  },
  "author": "archersdao",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "npm-ensure": "^1.2.0",
    "ts-jest": "^27.0.3",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "is-type-of": "^1.2.1",
    "lodash.get": "^4.4.2"
  }
}
